TABLE-3[Vehicle_Registration]

create table Vehicle_Registration(
vr_owner_name varchar2(20),
vr_state varchar2(20),
vr_vehicle_type varchar2(20),
vr_manufacturer varchar2(20),
vr_model varchar2(20),
vr_engine_number varchar2(20),
vr_yom varchar2(20),
vr_location varchar2(20),
vr_price float(20),
vr_dop varchar2(20),
p_id varchar2(20),
premium_amount float(20),
maturity_date varchar2(20),
vr_status_report varchar(20),
U_Id varchar2(20));

insert into Vehicle_Registration values('ankit','jh','tw','honda','suv','ea121',null,'kp','1000',null,'TW171000','100',null,null,null);


select * from Customer_Registration;

select * from Vehicle_Registration;

