
TABLE -1[Customer_Registration]


Create table Customer_Registration(Name varchar2(20),Password varchar2(20),Address varchar2(20),City varchar2(20),State varchar2(20),Country varchar2(20),Pin_code varchar2(20),Email
varchar2(20),Gender varchar2(20),Contact_No varchar2(20),DOB varchar2(20),U_ID varchar2(20));
insert into Customer_Registration values('admin','admin',null,null,null,null,null,null,null,null,null,'admin');
